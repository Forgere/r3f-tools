{
  "name": "r3f-tools",
  "version": "1.0.0",
  "description": "A collection of useful components and utilities for React Three Fiber",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "example": "vite examples --config examples/vite.config.ts",
    "example:build": "vite build examples --config examples/vite.config.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-three-fiber",
    "r3f",
    "three.js",
    "webgl",
    "3d",
    "instanced-mesh",
    "performance"
  ],
  "author": "Forgere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/forgere/r3f-tools.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "three": ">=0.178.0",
    "@react-three/fiber": "^8.15.12"
  },
  "devDependencies": {
    "@react-three/drei": "^10.6.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/three": "^0.158.3",
    "@react-three/fiber": "^8.15.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.178.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3"
  }
}