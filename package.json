{
	"name": "r3f-tools",
	"version": "1.0.6",
	"description": "A collection of useful components and utilities for React Three Fiber",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src",
		"README.md",
		"package.json",
		"LICENSE"
	],
	"exports": {
		".": {
			"import": "./dist/index.esm.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "vite build && tsc --project tsconfig.build.json",
		"dev": "vite build --watch",
		"preview": "vite preview",
		"example": "vite examples --config examples/vite.config.mjs",
		"example:build": "vite build examples --config examples/vite.config.mjs",
		"test": "jest",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint ./src/ --write && biome lint ./src/ --write",
		"lint:fix": "biome check --write",
		"format": "biome format ./src/ --write && biome format ./src/ --write",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"react-three-fiber",
		"r3f",
		"three.js",
		"webgl",
		"3d",
		"instanced-mesh",
		"performance"
	],
	"author": "Forgere",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/forgere/r3f-tools.git"
	},
	"peerDependencies": {
		"@react-three/fiber": "^9.3.0",
		"react": ">=19.1.1",
		"react-dom": ">=19.1.1",
		"three": ">=0.178.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@react-three/drei": "^10.6.1",
		"@react-three/fiber": "^9.3.0",
		"@types/jest": "^29.5.8",
		"@types/node": "^20.8.7",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.1",
		"@types/three": "^0.178.0",
		"@vitejs/plugin-react": "^4.3.4",
		"jest": "^29.7.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.7.1",
		"react-router-dom": "^7.7.1",
		"three": "^0.178.0",
		"typescript": "5.3",
		"vite": "^5.4.10",
		"vite-plugin-dts": "^4.5.4"
	},
	"dependencies": {
		"gsap": "^3.13.0",
		"leva": "^0.10.0"
	}
}
